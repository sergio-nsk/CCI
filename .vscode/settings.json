// Поместите параметры в этот файл, чтобы перезаписать параметры по умолчанию и пользовательские параметры.
{
    "code-runner.executorMap": {
        "cpp": "cd $dir && clang++ -std=c++14 -Wall -I. -I.. -g -pthread $fullFileName && ./a.out"
    },
    "files.associations": {
        "*.cpp": "cpp",
        "unordered_map": "cpp",
        "sstream": "cpp",
        "*.tcc": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "codecvt": "cpp",
        "condition_variable": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "functional": "cpp",
        "future": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "ratio": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "algorithm": "cpp",
        "xstddef": "cpp",
        "random": "cpp"
    },
    "python.linting.pylintArgs": [
        "--function-rgx=[a-z_][a-zA-Z0-9_]{2,30}$",
        "--module-rgx=",
        "--variable-rgx=[a-z_][a-zA-Z0-9_]{,30}$"
    ],
    "clang.cxxflags": [
        "-std=c++14"
    ],
    "C_Cpp.intelliSenseEngine": "Default",
    "[cpp]": {
        "editor.tabSize": 2,
        "editor.rulers": [
            80
        ]
    },
    "[c]": {
        "editor.tabSize": 2,
        "editor.rulers": [
            80
        ]
    }
}
