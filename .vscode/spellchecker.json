{
    "language": "en_US",
    "ignoreWordsList": [
        "stackSize",
        "nullptr",
        "typename",
        "cout",
        "const",
        "bool",
        "struct",
        "deque",
        "iostream"
    ],
    "documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "cpp"
    ],
    "ignoreRegExp": [
        "/\\\\(.*\\\\.(jpg|jpeg|png|md|gif|svg|pdf|PDF|SVG|JPG|JPEG|PNG|MD|GIF)\\\\)/g",
        "/((http|https|ftp|git)\\\\S*)/g",
        "/^(```\\\\s*)(\\\\w+)?(\\\\s*[\\\\w\\\\W]+?\\\\n*)(```\\\\s*)\\\\n*$/gm"
    ],
    "ignoreFileExtensions": [
        ".env"
    ],
    "checkInterval": 5000
}